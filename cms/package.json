{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@ckeditor/ckeditor5-build-classic": "^23.1.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@date-io/date-fns": "1.3.13",
    "@loadable/component": "^5.13.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "date-fns": "2.15.0",
    "lodash-es": "^4.17.15",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-catch": "^1.3.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "typeface-roboto": "^0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint '**/*.js'",
    "lint:css": "stylelint '**/*.js'",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js' --gzip"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.16",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^1.19.1",
    "stylelint": "^13.7.2",
    "source-map-explorer": "^2.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\w+/async-components/\\w+",
      "\\w+/containers/\\w+",
      "\\w+/const/\\w+",
      "\\w+/init/\\w+",
      "\\w+/theme/\\w+",
      "\\w+/actions.js",
      "\\w+/sagas.js",
      "setupTests.js",
      "serviceWorker.js",
      "index.js",
      "core/state/configureStore.js",
      "core/state/rootReducer.js",
      "core/state/rootSagas.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
